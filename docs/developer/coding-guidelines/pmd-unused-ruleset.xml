<?xml version="1.0" encoding="UTF-8"?>
<ruleset name="RuyiSDK Unused Code Rules"
  xmlns="http://pmd.sourceforge.net/ruleset/2.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://pmd.sourceforge.net/ruleset/2.0.0 https://pmd.github.io/pmd-7.0.0/ruleset_xml_schema.xsd"
  xmlns:description="http://pmd.sourceforge.net/ruleset/2.0.0">

  <description>Fail the build for unused Java variables, private members, and parameters.</description>

  <rule ref="category/java/bestpractices.xml/UnusedLocalVariable"/>
  <rule ref="category/java/bestpractices.xml/UnusedPrivateField"/>
  <rule ref="category/java/bestpractices.xml/UnusedPrivateMethod"/>
  <rule ref="category/java/bestpractices.xml/UnusedFormalParameter"/>
</ruleset>
